{
  "name": "outbox-pattern-poc",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/fan-h-hoshino/outbox-pattern-poc#readme",
  "bugs": {
    "url": "https://github.com/fan-h-hoshino/outbox-pattern-poc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fan-h-hoshino/outbox-pattern-poc.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -f .env -- tsx watch src/index.ts",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "dotenvx run -f .env -- prisma generate",
    "db:push": "dotenvx run -f .env -- prisma db push",
    "db:studio": "dotenvx run -f .env -- prisma studio",
    "pubsub:setup": "dotenvx run -f .env -- tsx src/scripts/setup-pubsub.ts",
    "lint": "eslint .",
    "load-test": "k6 run k6/publish-load-test.js",
    "streaming-load-test": "k6 run k6/streaming-publish-load-test.js",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.2",
    "@google-cloud/pubsub": "^5.2.0",
    "@hono/node-server": "^1.19.7",
    "@hono/zod-validator": "^0.7.6",
    "@prisma/adapter-mariadb": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "hono": "^4.11.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  }
}
